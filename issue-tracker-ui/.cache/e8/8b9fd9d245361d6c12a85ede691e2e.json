enerated":{"line":114,"column":34},"source":"app/NumInput.js","original":{"line":52,"column":21}},{"name":"onChange","generated":{"line":115,"column":0},"source":"app/NumInput.js","original":{"line":54,"column":2}},{"name":"onChange","generated":{"line":115,"column":2},"source":"app/NumInput.js","original":{"line":54,"column":2}},{"generated":{"line":115,"column":10},"source":"app/NumInput.js","original":{"line":54,"column":10}},{"name":"PropTypes","generated":{"line":115,"column":12},"source":"app/NumInput.js","original":{"line":54,"column":12}},{"name":"func","generated":{"line":115,"column":31},"source":"app/NumInput.js","original":{"line":54,"column":22}},{"generated":{"line":115,"column":35},"source":"app/NumInput.js","original":{"line":54,"column":12}},{"name":"isRequired","generated":{"line":115,"column":36},"source":"app/NumInput.js","original":{"line":54,"column":27}},{"generated":{"line":116,"column":0},"source":"app/NumInput.js","original":{"line":52,"column":21}},{"generated":{"line":116,"column":1},"source":"app/NumInput.js","original":{"line":52,"column":0}}],"sources":{"app/NumInput.js":"import PropTypes from 'prop-types';\nimport React from 'react';\n\nexport default class NumInput extends React.Component {\n\n  constructor(props) {\n    super(props);\n    this.state = { value: this.format(props.value) };\n    this.onBlur = this.onBlur.bind(this);\n    this.onChange = this.onChange.bind(this);\n  }\n\n  componentWillReceiveProps(newProps) {\n    this.setState({ value: this.format(newProps.value) });\n  }\n\n  onBlur(e) {\n    console.log('==> onBlur dans NumImput');\n    this.props.onChange(e, this.unformat(this.state.value));\n  }\n\n  // On ne change le state que si la valeur est valide!!!\n  // cad un nombre\n  onChange(e) {\n    if (e.target.value.match(/^\\d*$/)) {\n      this.setState({ value: e.target.value });\n    }\n  }\n\n  // Conversion en string pour l'affichage\n  format(num) {\n    return num != null ? num.toString() : '';\n  }\n\n  // Conversion dans le type naturel pour le state parent\n  unformat(str) {\n    const val = parseInt(str, 10);\n    return isNaN(val) ? null : val;\n  }\n\n  render() {\n    // on utilise un spread attribute pour récupérer les propriétés \n    // spécifiés par le parent comme la taille du champ (size={5})\n    return (\n      <input type=\"text\" {...this.props} value={this.state.value} \n        onBlur={this.onBlur} onChange={this.onChange}\n      />\n    );\n  }\n}\n\nNumInput.propTypes = {\n  value: PropTypes.number,\n  onChange: PropTypes.func.isRequired,\n};"},"lineCount":null}},"error":null,"hash":"6bffa72cf87d2d112645b1f3d4a1433c","cacheData":{"env":{}}}